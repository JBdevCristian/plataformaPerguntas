<!DOCTYPE html>
<html lang="pt-br">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <link rel="stylesheet" href="/css/style.css">
    <title>Guia Perguntas</title>
</head>
    <body>
        <img src="/img/paypal.png" style="width: 250px;" alt="">
        <h1>Guia Perguntas!</h1>
        <hr>
        <h3>Lista de perguntas</h3>

        <P>Nome: <%= nome %></P> 
        <p>Linguagem favorita: <%= lang %></P> 
        <p>Empresa: <%= empresa %> </p>
        <p>Numero de Inscritos: <%= inscritos %></p>

        <% if(msg) { %>
            <h3>Isso Ã© uma mensagem de erro!</h3>
        <%} else { %> 
            <h3>Nenhume Erro!</h3>
            <%} %>

        <% produtos.forEach(function(produto) { %>
            <hr>
            <h3><%= produto.nome %></h3>
            <h4><%= produto.preco %></h4>
        <% }) %>

       
        


    </body>
</html>